stinc="-I../tinyscheme/ -I../Box2D/Include/ -I../Box2D/Examples/TestBed/Framework/ -I/Library/Frameworks/SDL.framework/Headers/"

cp ../Box2D/Build/Xcode/build/Debug/libBox2d.dylib .
g++ main.cpp -c -g $stinc
g++ script.cpp -c -g $stinc
g++ input.cpp -c -g $stinc
g++ player.cpp -c -g $stinc
g++ systemmanager.cpp -c -g $stinc
g++ ploy/symbol.cpp -c -g -Iploy/
g++ ../tinyscheme/scheme.c -c -g -I../tinyscheme/ -DUSE_STRLWR=1 -DOSX -DSTANDALONE=0 -DUSE_DL=0
g++ ../Box2D/Examples/TestBed/Framework/Render.cpp -c -g $stinc -D__APPLE__
g++ -g main.o scheme.o Render.o script.o input.o player.o systemmanager.o symbol.o -framework OpenGL -framework GLUT -framework SDL -framework IOKit -L../Box2D/Build/Xcode/build/Debug -lBox2D -o SurfaceTension `sdl-config --cflags`
etags main.cpp /System/Library/Frameworks/GLUT.framework/Headers/glut.h /System/Library/Frameworks/OpenGL.framework/Headers/gl.h ../Box2D/Source/Common/*.h ../Box2D/Source/Collision/*.h ../Box2D/Source/Dynamics/*.h